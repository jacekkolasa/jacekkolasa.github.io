{
  "name": "parental-tools",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "analyze": "react-static build --analyze",
    "serve": "serve dist -p 3000",
    "lint": "eslint .",
    "test": "jest",
    "checkAll": "yarn lint && yarn test",
    "deploy": "gh-pages -d dist -b master"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "classnames": "^2.2.6",
    "d3": "^5.9.2",
    "formik": "^1.5.7",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "papaparse": "^5.0.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-datepicker": "^2.7.0",
    "react-dom": "^16.8.6",
    "react-measure": "^2.3.0",
    "react-router-dom": "^5.0.0",
    "react-static": "^7.1.0",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "prop-types": "^15.7.2",
    "react-static-plugin-css-modules": "^7.1.0",
    "react-static-plugin-react-router": "^7.0.10",
    "react-static-plugin-typescript": "^7.0.10",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "autoprefixer": "~9.5.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/utils/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/utils/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(txt)$": "<rootDir>/src/utils/jestTxtTransformer.js"
    }
  }
}
